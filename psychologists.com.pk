server {
    listen 80;
    server_name pscychologists.com.pk www.psychologists.com.pk;

    location / {
        include proxy_params;
        proxy_pass http://unix:/var/www/Psycologist_Consultant_Flask/app.sock;
    }

    error_log /var/log/nginx/psychologists_error.log;
    access_log /var/log/nginx/psychologists_access.log;
}
